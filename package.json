{
  "name": "recipes",
  "version": "1.0.0",
  "description": "Real Italian Recipes",
  "scripts": {
    "start": "npx tailwindcss -i ./src/style.css -o ./src/public/style.css --watch & npx five-server",
    "build": "npx tailwindcss -i ./src/style.css -o ./src/public/style.css",
    "deploy": "gh-pages-deploy"
  },
  "gh-pages-deploy": {
    "staticpath": "src/public",
    "cname": "realitalianrecipes.com",
    "prep": [
      "build"
    ],
    "post": [
    ],
    "noprompt": false
  },
  "author": "Matteo Merola <mattmezza@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "five-server": "^0.1.4",
    "gh-pages-deploy": "^0.5.1",
    "tailwindcss": "^3.1.4"
  }
}
